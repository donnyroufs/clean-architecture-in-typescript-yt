{
  "name": "app",
  "version": "0.0.0",
  "main": "main.ts",
  "author": "Donny Roufs",
  "scripts": {
    "dev": "ts-node-dev --respawn  src/main.ts",
    "test": "jest --verbose",
    "test:cov": "jest --verbose --collectCoverage",
    "build": "tsc",
    "test:watch": "jest --watch --verbose",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "express": "^4.18.2",
    "jest-mock-extended": "^2.0.7"
  },
  "devDependencies": {
    "@jest-decorated/core": "^0.1.7",
    "@types/express": "^4.17.20",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.22.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-extended": "^3.2.4",
    "lint-staged": ">=13",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn eslint",
      "yarn prettier --write",
      "yarn test --watchAll=false --findRelatedTests --bail"
    ]
  }
}
